<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--检查的圆环-->
    <declare-styleable name="UpdateProgressBar">


        <!--公共属性-->
        <!--进度宽度-->
        <attr name="progress_width" format="dimension" />
        <!--起始角度-->
        <attr name="start_angle" format="integer" />
        <!--动画时间-->
        <attr name="cover_animduration_time" format="integer" />

        <!--percentCover 模式特有属性-->
        <!--进度起始色-->
        <attr name="cover_start_color" format="color" />
        <!--背景起始色-->
        <attr name="cover_bg_cir_color" format="color" />

        <!--percentSplice 模式特有属性-->
        <!--暂无-->

    </declare-styleable>

</resources>